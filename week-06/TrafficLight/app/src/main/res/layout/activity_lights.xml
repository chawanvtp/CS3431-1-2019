<?xml version="1.0" encoding="utf-8"?>
<android.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent" android:id="@+id/linearLayout">

    <ImageView
            android:layout_width="165dp"
            android:layout_height="wrap_content" app:srcCompat="@drawable/red" android:id="@+id/imageView"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="288dp" android:layout_marginStart="16dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.007"/>
    <ImageView
            android:layout_width="165dp"
            android:layout_height="wrap_content" app:srcCompat="@drawable/red"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" android:layout_marginStart="120dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintHorizontal_bias="0.025" android:id="@+id/imageView2"/>
    <ImageView
            android:layout_width="165dp"
            android:layout_height="wrap_content" app:srcCompat="@drawable/red" android:id="@+id/imageView3"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="120dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintHorizontal_bias="0.025"
            android:layout_marginBottom="16dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <ImageView
            android:layout_width="165dp"
            android:layout_height="wrap_content" app:srcCompat="@drawable/red" android:id="@+id/imageView4"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="288dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.007"/>
</android.constraintlayout.widget.ConstraintLayout>